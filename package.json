{
  "name": "@syfxlin/eslint-config",
  "type": "module",
  "version": "0.0.0-semantic-release",
  "description": "Otstar's ESLint config",
  "author": "Otstar Lin <i@ixk.me> (https://github.com/syfxlin)",
  "license": "MIT",
  "homepage": "https://github.com/syfxlin/eslint-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syfxlin/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/syfxlin/eslint-config/issues"
  },
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bunchee",
    "dev": "bunchee --watch",
    "dev:viewer": "eslint-flat-config-viewer",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0"
  },
  "dependencies": {
    "@antfu/eslint-config": "3.8.0",
    "@eslint-react/eslint-plugin": "^1.5.22",
    "@unocss/eslint-plugin": "^0.61.2",
    "eslint-plugin-format": "^0.1.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-svelte": "^2.41.0",
    "svelte-eslint-parser": "^0.39.2"
  },
  "devDependencies": {
    "bunchee": "^5.2.2",
    "eslint": "^9.6.0",
    "eslint-flat-config-viewer": "^0.1.20"
  }
}
