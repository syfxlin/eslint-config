{
  "name": "@syfxlin/eslint-config",
  "type": "module",
  "version": "1.0.0",
  "description": "Otstar's ESLint config",
  "author": "Otstar Lin <i@ixk.me> (https://github.com/syfxlin)",
  "license": "MIT",
  "homepage": "https://github.com/syfxlin/eslint-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syfxlin/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/syfxlin/eslint-config/issues"
  },
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "dev:viewer": "eslint-flat-config-viewer",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "peerDependencies": {
    "eslint": ">=8.40.0"
  },
  "dependencies": {
    "@antfu/eslint-config": "^2.6.2",
    "@unocss/eslint-plugin": "^0.58.3",
    "eslint-plugin-format": "^0.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-svelte": "^2.35.1",
    "svelte-eslint-parser": "^0.33.1"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-flat-config-viewer": "^0.1.8",
    "tsup": "^8.0.1"
  }
}
